<!--- The wireframe skin class for the border of the MX ToolTip component. 

@see mx.controls.ToolTip

@langversion 3.0
@playerversion Flash 10
@playerversion AIR 1.5
@productversion Flex 4
-->
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
                        >
    
    <s:states>
        <s:State name="normal"/>
        <s:State name="disabled"/>
    </s:states>
    
    <!-- shadow -->
    <s:Rect left="1" right="1" top="1" bottom="1" radiusX="3" radiusY="3">
        <s:fill>
            <s:SolidColor color="#333333" alpha=".15"/>
        </s:fill>
    </s:Rect>
    
    <!--- @private -->
    <s:Rect id="border" left="0" right="2" top="0" bottom="2" radiusX="2" radiusY="2">
        <s:stroke>            
            <!--- @private -->
            <s:SolidColorStroke id="borderStroke" color="#707070" />
        </s:stroke>
        <s:fill>
            <!--- @private -->
            <s:SolidColor id="bgFill" color="#FFFFFF"/>
        </s:fill>
    </s:Rect>
    
    
</s:Skin>