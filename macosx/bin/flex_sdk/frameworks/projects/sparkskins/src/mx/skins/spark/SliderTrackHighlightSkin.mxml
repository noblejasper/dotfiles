<!--- The Spark skin class for the track highlight of the MX Slider component. 

      @see mx.controls.sliderClasses.Slider
        
      @langversion 3.0
      @playerversion Flash 10
      @playerversion AIR 1.5
      @productversion Flex 4
-->
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" 
            height="11" width="200" disabledAlpha="0.5">
    
    <s:Rect left="0" right="0" top="2" height="4" radiusX="2">
        <s:fill>
            <s:SolidColor color="{getStyle('accentColor')}" alpha="0.35" />
        </s:fill>
    </s:Rect>
</s:Skin>
