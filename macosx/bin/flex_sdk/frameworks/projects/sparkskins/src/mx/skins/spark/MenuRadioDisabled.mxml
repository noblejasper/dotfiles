<!--- The Spark skin class for the disabled radio icon of the MX Menu component. 

      @see mx.controls.Menu

@langversion 3.0
@playerversion Flash 10
@playerversion AIR 1.5
@productversion Flex 4
-->
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" 
    alpha=".5">

    <fx:Script>
        /* Define the symbol fill items that should be colored by the "symbolColor" style. */
        static private const symbols:Array = ["radioFill"];
        /**
         * @private
         */
        override public function get symbolItems():Array {return symbols};
    </fx:Script>
    
    <!--- @private -->
    <s:Ellipse id="radio" width="5" height="5">
        <s:fill>
            <!--- @private -->
            <s:SolidColor color="0" id="radioFill"/>
        </s:fill>
    </s:Ellipse>
</s:SparkSkin>

